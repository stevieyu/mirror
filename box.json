{
    "main": "index.php",
    "stub": false,
    "output": "mirror.phar",
    "compression": "GZ",
    "finder": [
      {
        "name": "/.*\\.php|.*\\.json|.*\\.yaml|.*\\.xml/",
        "exclude": [
          "doc",
          "test",
          "test_old",
          "tests",
          "Tests",
          "vendor-bin"
        ],
        "in": "vendor"
      }
    ],
    "compactors": [
      "KevinGH\\Box\\Compactor\\Php",
      "KevinGH\\Box\\Compactor\\Json"
    ]
  }